<?xml version="1.0" encoding="UTF-8" ?>
<environment xmlns="http://drivebuild.com"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://drivebuild.com ../drivebuild.xsd">
    <author>Stefan Huber</author>

    <lanes>
        <lane>
            <laneSegment x="0" y="0" width="5"/>
            <laneSegment x="50" y="0" width="5"/>
            <laneSegment x="80" y="20" width="5"/>
            <laneSegment x="100" y="20" width="5"/>
        </lane>
    </lanes>

    <obstacles>
        <cube x="105" y="25" width="1" length="10" height="8"/>
        <cylinder x="60" y="-10" radius="5" height="7"/>
        <cone x="40" y="10" height="10" baseRadius="6"/>
        <bump x="20" y="-8" width="1.5" length="5" height="0.1" upperLength="4.5" upperWidth="1"/>
    </obstacles>
</environment>
